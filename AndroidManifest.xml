<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.onurgunes.anket"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.onurgunes.anket.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.onurgunes.anket.AddQuestion"
            android:label="@string/title_activity_add_question" >
        </activity>
        <activity
            android:name="com.onurgunes.anket.DeleteQuestion"
            android:label="@string/title_activity_delete_question" >
        </activity>
        <activity
            android:name="com.onurgunes.anket.ShowStatistics"
            android:label="@string/title_activity_show_statistics" >
        </activity>
        <activity
            android:name="com.onurgunes.anket.ChooseQuestions"
            android:label="@string/title_activity_choose_questions" >
        </activity>
        <activity
            android:name="com.onurgunes.anket.Survey"
            android:label="@string/title_activity_survey" >
        </activity>
        <activity
            android:name="com.onurgunes.anket.ImportExportExcel"
            android:label="@string/title_activity_import_export_excel" >
        </activity>
    </application>

</manifest>
